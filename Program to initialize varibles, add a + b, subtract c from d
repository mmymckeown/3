int main() {
    // Initialize variables
    int a = 10;
    int b = 20;
    int c = 30;

    // Add values of a and b
    int d = a + b;
    // Print value of variable d
    printf("Value of d (a + b): %d\n", d);

    // Subtract variable c from d
    int e = d - c;
    // Print value of variable e
    printf("Value of e (d - c): %d\n", e);

    return 0;
}
